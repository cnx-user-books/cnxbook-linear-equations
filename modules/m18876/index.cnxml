</exercise><exercise id="eip-622"><problem id="eip-181">
		<para id="eip-211"><m:math>
		<m:semantics>
			<m:mrow>
				<m:mn>8</m:mn><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
