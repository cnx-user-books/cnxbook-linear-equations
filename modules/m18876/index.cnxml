</exercise><exercise id="eip-291"><problem id="eip-237">
		<para id="eip-649">
    <m:math>
 <m:semantics>
  <m:mrow>
   <m:mn>5</m:mn><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>25</m:mn>
